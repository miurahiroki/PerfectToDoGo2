<h1>画像</h1>

<table>
  <thead>
    <tr>
      <th>画像名</th>
      <th>詳細内容</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.name %></td>
        <td><%= photo.content %></td>
        <td><%= link_to '詳細', photo %></td>
        <td><%= link_to '編集', edit_photo_path(photo) %></td>
        <td><%= link_to '削除', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

